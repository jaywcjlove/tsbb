{
  "name": "@template/react-component",
  "version": "3.7.1",
  "private": true,
  "description": "React Component Example.",
  "scripts": {
    "watch": "tsbb watch --entry src/index.jsx",
    "build": "tsbb build --entry src/index.jsx --no-source-maps&& npm run css:build && npm run css:build:dist",
    "css:build": "compile-less -d src -o esm",
    "css:watch": "compile-less -d src -o esm --watch",
    "css:build:dist": "compile-less -d src --combine dist/markdown-editor.css --rm-global",
    "test": "tsbb test --env=jsdom",
    "coverage": "tsbb test --env=jsdom --coverage --bail",
    "start": "parcel website/index.html",
    "doc": "parcel build website/index.html --dist-dir doc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/tsbb/tree/master/example/react-component"
  },
  "files": [
    "typings",
    "esm",
    "lib"
  ],
  "keywords": [],
  "license": "MIT",
  "source": "src/index.js",
  "peerDependencies": {
    "react": ">=16.7.0",
    "react-dom": ">=16.7.0"
  },
  "devDependencies": {
    "@babel/runtime": "7.16.7",
    "@parcel/transformer-less": "2.3.2",
    "compile-less-cli": "1.8.11",
    "less": "4.1.2",
    "parcel": "2.3.2",
    "react-test-renderer": "17.0.2",
    "tsbb": "^3.7.1"
  },
  "dependencies": {
    "@parcel/transformer-react-refresh-wrap": "2.3.2",
    "classnames": "2.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  }
}
