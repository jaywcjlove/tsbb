{
  "name": "@template/react-component",
  "version": "4.0.3",
  "private": true,
  "description": "React Component Example.",
  "scripts": {
    "watch": "tsbb watch src/*.{jsx,js} --useBabel",
    "build": "tsbb build src/*.{jsx,js} --useBabel && npm run css:build && npm run css:build:dist",
    "css:build": "compile-less -d src -o esm",
    "css:watch": "compile-less -d src -o esm --watch",
    "css:build:dist": "compile-less -d src --combine dist/markdown-editor.css --rm-global",
    "test": "tsbb test",
    "coverage": "tsbb test --coverage --bail --updateSnapshot",
    "start": "parcel website/index.html",
    "doc": "parcel build website/index.html --dist-dir doc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/tsbb/tree/master/example/react-component"
  },
  "files": [
    "typings",
    "esm",
    "lib"
  ],
  "keywords": [],
  "license": "MIT",
  "source": "src/index.js",
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/build/"
    ]
  },
  "peerDependencies": {
    "react": ">=16.7.0",
    "react-dom": ">=16.7.0"
  },
  "devDependencies": {
    "@babel/runtime": "~7.21.0",
    "@parcel/transformer-less": "~2.8.3",
    "compile-less-cli": "~1.8.13",
    "less": "~4.1.3",
    "parcel": "~2.8.3",
    "react-test-renderer": "~18.2.0",
    "tsbb": "4.0.3"
  },
  "dependencies": {
    "@parcel/transformer-react-refresh-wrap": "~2.8.3",
    "classnames": "~2.3.2",
    "react": "~18.2.0",
    "react-dom": "~18.2.0"
  }
}
