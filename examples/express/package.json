{
  "name": "@template/express",
  "version": "4.4.0",
  "private": true,
  "description": "Express Example.",
  "scripts": {
    "start": "npm run build && node lib/app.js",
    "dev:ts": "ts-node-dev --inspect -- src/app.ts",
    "dev": "npm run build && nodemon --inspect lib/app.js",
    "watch": "tsbb watch",
    "build": "tsbb build --bail",
    "test": "tsbb test",
    "coverage": "tsbb test --coverage --bail"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/tsbb/tree/master/example/express"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "node_modules/*"
    ],
    "delay": "2500"
  },
  "devDependencies": {
    "@types/compression": "~1.7.2",
    "@types/cookie-parser": "~1.4.2",
    "@types/express": "~4.17.13",
    "@types/http-errors": "~2.0.1",
    "nodemon": "~2.0.22",
    "ts-node-dev": "~2.0.0",
    "tsbb": "4.4.0"
  },
  "dependencies": {
    "compression": "~1.7.4",
    "cookie-parser": "~1.4.6",
    "ejs": "~3.1.6",
    "express": "~4.18.0",
    "http-errors": "~2.0.0"
  }
}
