{
  "name": "tsbb",
  "version": "0.0.6",
  "description": "Zero-config CLI for TypeScript node.js application development.",
  "bin": {
    "tsbb": "./lib/command/tsbb.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/tsbb.git"
  },
  "bugs": {
    "url": "https://github.com/jaywcjlove/tsbb/issues"
  },
  "homepage": "https://github.com/jaywcjlove/tsbb",
  "scripts": {
    "prepare": "npm run build",
    "type-check": "tsc --noEmit",
    "build": "npm run tsc",
    "build:types": "tsc -p ./ -t es2015 --types --emitDeclarationOnly --outDir lib",
    "tsc": "tsc -p ./ --types --outDir lib",
    "start": "npm run tsc -- --watch"
  },
  "files": [
    "lib",
    "bin"
  ],
  "keywords": [
    "typescript",
    "babel",
    "node.js"
  ],
  "author": "kenny wang <wowohoo@qq.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.4.4",
    "chokidar": "^3.0.0",
    "colors-cli": "^1.0.23",
    "core-js": "^3.0.1",
    "execa": "^1.0.0",
    "fs-extra": "^7.0.1",
    "ora": "^3.4.0",
    "request": "^2.88.0",
    "typescript": "^3.4.5",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.1",
    "@types/execa": "^0.9.0",
    "@types/fs-extra": "^5.0.5",
    "@types/node": "^11.13.8",
    "@types/request": "^2.48.1",
    "@types/yargs": "^13.0.0"
  }
}
